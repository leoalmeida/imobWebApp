{
  "name": "Imobapp",
  "description": "Gestão de imobiliária",
  "version": "0.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "oauth2": {
    "client_id": "953449888791-u28ifn1c5ufm1k2omfc783tp69a55a1g.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "icons": {
    "128": "img/gato.png"
  },
  "key": "",
  "browser_action": {
    "default_title": "Imobcat",
    "default_icon": "img/gato.png"
  },  
  "background": {
       "scripts": ["background.js"],
       "persistent": false
  },
  "content_security_policy": "script-src 'self' https://viacep.com.br/ws/ filesystem 'unsafe-eval'; object-src 'self'",  
  "permissions": [
    "identity",
    "https://ssl.gstatic.com/",
    "https://www.googleapis.com/",
    "https://accounts.google.com/",
    "history",
    "tabs",
    "<all_urls>"
  ]
}